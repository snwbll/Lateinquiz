        TextInput:
            id: zweitesg
            background_color: root.colorZSG
            text: root.zweitepssg
            size_hint: 0.28, 0.05
            pos_hint: {'center_x': 0.78, 'center_y': 0.73}
        TextInput:
            id: drittesg
            background_color: root.colorDSG
            text: root.drittepssg
            size_hint: 0.28, 0.05
            pos_hint: {'center_x': 0.78, 'center_y': 0.62}
        TextInput:
            id: erstepl
            background_color: root.colorEPL
            text: root.erstepspl
            size_hint: 0.28, 0.05
            pos_hint: {'center_x': 0.78, 'center_y': 0.51}
