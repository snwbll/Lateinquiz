            font_size: min(root.height, root.width) / 15
            background_color: COLOR(GREEN)
            on_release: root.get_next()

<QuizPopup>:
    size_hint: .8, .8
    title: "Lösung"
    title_size: root.height * .05
    seperator_color: COLOR(GREEN)
    auto_dissmiss: False
    message: message
    wrapped_button: wrapped_button
    BoxLayout:
        orientation: "vertical"
        padding: root.width * .02, root.height * .02
        spacing: min(root.width, root.height) * .02
        Label:
            id: message
            font_size: min(self.height, self.width) / 8
            text: "Antworttext"
            halign: "center"
            markup: True
            text_size: self.width, None
            center_y: .5
        NormalButton:
            id: wrapped_button
            text: "Schließen"
            size_hint: 1, None
            height: root.height / 8
            on_release: root.dismiss()

<InfoScreen@Screen>:
    FloatLayout:
        Label:
			text: app.getText()
			halign: "center"
			pos_hint: {"center_x": .5, "center_y": .6}
			markup: True
			font_size: root.height / 24
			text_size: self.width, None
			on_ref_press: app.on_ref_press(*args)
	    Label:
	        text: app.getPlaystorelink()
	        halign: "center"
	        pos_hint: {"center_x": .5, "center_y": .15}
			markup: True
			font_size: root.height / 24
			text_size: self.width, None
			on_ref_press: app.on_ref_press(*args)

<AboutScreen@Screen>:
	BoxLayout:
	    orientation: "vertical"
		padding: root.width * .02, root.height * .02
		spacing: min(root.width, root.height) * .02
		Label:
		    markup: True
		    font_size: min(root.height, root.width) / 10
		    text: "[b]Über diese App[/b]"
		NormalButton:
		    text: "Info"
            background_color: COLOR(GREEN)
            on_release: app.root.changeScreen(self.text.lower())
        NormalButton:
            text: "How to"
            on_press: root.display_help_screen()
        NormalButton:
            text: "Statistiken"
            background_color: COLOR(RED)
            on_release: app.root.changeScreen(self.text.lower())

