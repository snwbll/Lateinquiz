#:import COLOR kivy.utils.get_color_from_hex
#:set RED "#F94A69"
#:set GREEN "#06A5AB"
#:set BLUE "#278FDD"
#:set NONE "#162038"

<CheckBox@CheckBox>:
    size_hint: 0.2, 0.07

<TextInput@TextInput>:
    markup: True

<WrappedLabel@Label>:
	size_hint_y: None
	height: self.texture_size[1] + (self.texture_size[1]/0.6)
	markup: True
    font_name: "Roboto"

<LearnLabel@Label>:
    size_hint_y: None
	height: self.texture_size[1] + (self.texture_size[1]/2)
	markup: True
    font_name: "Roboto"

<NormalButton@Button>:
    font_name: "Roboto"
    font_size: min(self.height, self.width) / 5
    bold: True
    background_normal: "./images/button_normal.png"
    background_down: "./images/button_down.png"
    background_color: COLOR(BLUE)
    border: (2, 2, 2, 2)

<KivyLateinRoot>:
	orientation: "vertical"
	quiz_screen: quiz_screen

	ScreenManager:
		id: screen_manager
		StartScreen:
			name: "start_screen"
		AboutScreen:
			id: about_screen
			name: "about_screen"
		QuizScreen:
		    id: quiz_screen
		    name: "quiz_screen"
		VokabelnScreen:
		    id: vokabeln_screen
		    name: "vokabeln_screen"
		GrammatikScreen:
		    id: grammatik_screen
		    name: "grammatik_screen"
        StatisticsScreen:
            id: statistics_screen
            name: "statistics_screen"
        InfoScreen:
            id: info_screen
            name: "info_screen"
        GrammatikauswahlScreen:
            id: grammatik_auswahl_screen
            name: "grammatikauswahl_screen"
        KonjugationsScreen:
            id: konjugations_screen
            name: "konjugations_screen"
        PPAScreen:
            id: ppa_screen
            name: "ppa_screen"

<StartScreen@Screen>:
	BoxLayout:
		# startscreen für die auswahl
		orientation: "vertical"
		padding: root.width * .02, root.height * .02
		spacing: min(root.width, root.height) * .02
		WrappedLabel:
			text: "[b]Latein Vokabeltrainer[/b]"
			font_size: min(root.height, root.width) / 10
		NormalButton:
			text: "Quiz"
		    on_release: app.root.changeScreen(self.text.lower())
		NormalButton:
			text: "Vokabeln"
			background_color: COLOR(GREEN)
		    on_release: app.root.changeScreen(self.text.lower())
		NormalButton:
			text: "Grammatik"
			background_color: COLOR(GREEN)
		    on_release: app.root.changeScreen(self.text.lower())
		NormalButton:
			text: "Über diese App"
			background_color: COLOR(RED)
			on_release: app.root.changeScreen("about")

<GrammatikauswahlScreen@Screen>:
    BoxLayout:
        orientation: "vertical"
        padding: root.width * .02, root.height * .02
		spacing: min(root.width, root.height) * .02
        WrappedLabel:
			text: "[b]Grammatik[/b]"
			font_size: min(root.height, root.width) / 10
        NormalButton:
		    text: "Deklinationen"
		    background_color: COLOR(BLUE)
		    on_release: app.root.changeScreen(self.text.lower())
        NormalButton:
            text: "Konjugationen"
            background_color: COLOR(GREEN)
            on_release: app.root.changeScreen("konjugationen")
        NormalButton:
            text: "PPA"
            background_color: COLOR(RED)
            on_release: app.root.changeScreen("ppa")

